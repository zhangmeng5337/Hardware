/**
  @brief DFU jump setup 
*/
void JumpToBootloader(void) {  
  
  GPIO_InitTypeDef GPIO_InitStruct;
  HAL_GPIO_WritePin(GPIOA, GPIO_PIN_11|GPIO_PIN_12, GPIO_PIN_RESET);
  /**USB GPIO Configuration    
  PA11     ------> USB_DM
  PA12     ------> USB_DP 
  */
  GPIO_InitStruct.Pin = GPIO_PIN_11|GPIO_PIN_12;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_PULLDOWN;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOA, &GPIO_InitStruct);
  HAL_Delay(1000);  
  USBD_Stop(&hUsbDeviceFS);
  HAL_Delay(1000);  
  USBD_DeInit(&hUsbDeviceFS);
  HAL_Delay(1000);
  
  *((unsigned long *)0x2001FFF1) = 0xDEADBEEF;
  NVIC_SystemReset();
}

/**
  @brief DFU jump manager
*/
void __bootloader_manager(void)
{
  // function pointer
  void (*SysMemBootJump)(void);

  // Add this to SystemInit() in system_clocks.c
  if ( *((unsigned long *)0x2001FFF1) == 0xDEADBEEF) {

    // reset the trigger
    *((unsigned long *)0x2001FFF1) =  0xCAFEFEED; 

    __HAL_SYSCFG_REMAPMEMORY_SYSTEMFLASH();

    const uint32_t p = (*((uint32_t *) 0x1FFF0000));
    __set_MSP( p );

    void (*SysMemBootJump)(void);
    SysMemBootJump = (void (*)(void)) (*((uint32_t *) 0x1FFF0004));
    SysMemBootJump();
    while (42);
  }
}
